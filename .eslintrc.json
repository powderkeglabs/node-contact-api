{
    "extends": "google",
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": 2,
      "no-unused-vars": 0,
      "new-cap": 0,
      "camelcase": 0
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "globals": {
      "chai": true,
      "assert": true
    }
}
